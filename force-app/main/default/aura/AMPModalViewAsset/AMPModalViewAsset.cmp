                    </div>
                </div>
            </div>
                
         </div>
        <aura:if isTrue="{!v.pages != 0}"> 
           <div class="assetpaginate" style="float:right;">
               <button type="button" style="width:36px;height:36px;" class="{!v.page == 0?'disablecolor':'paginationbtncolor'}"  id="button1" disabled="{!v.page == 0}" alt="&lt;" label="&lt;" onclick="{! c.prev }" >&lt;</button>
               <button  type="button" style="width:36px;height:36px;" class="{!and((v.page == 0) ,(v.pages == '0'))? 'disablecolor': 'paginationbtncolor'}" id="button2" title="{!'Total of '+(v.rowcount)+' record in '+v.pages+' pages'}"  disabled="{!and((v.page == 0) ,(v.pages == '0'))}"  label="{!v.currentpage}" >{!v.currentpage}</button>
               <button type="button" style="width:36px;height:36px;" class="{!(v.page == (v.pages-1))?'disablecolor':'paginationbtncolor'}" id="button3" disabled="{!(v.page == (v.pages-1)) || (v.pages == '0')}"  label="&gt;" onclick="{! c.next }" > &gt;</button>                   
           </div>
        </aura:if>
        
    </div>
</aura:component>